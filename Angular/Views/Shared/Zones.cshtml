@model List<WS.Models.OUT.Contenu>


@if ((Model != null) && (Model.Count > 0))
{

    foreach (WS.Models.OUT.Contenu _Current in Model)
    {
        <div class="zone">
            <a href="/Content/Images/Contenus/@_Current.Id/@_Current.Full" target="_blank" title="Cliquer pour agrandir"><img class="photo" src="/Content/Images/Contenus/@_Current.Id/@_Current.Full" /></a>
            <div class="titre">@_Current.Titre</div>
            <div class="texte">@Html.Raw(_Current.Texte.ToBR())</div>
        </div>
    }

}


<script language="javascript" src="@Url.Content("~/Areas/Contenus/Scripts/Views/Shared/Zones.js")" type="text/javascript"></script>